{
  "name": "winston",
  "version": "1.0.0",
  "description": "Your personal assistant to all things blockchain.",
  "private": true,
  "main": "public/electron.js",
  "author": "michaeladennis@yahoo.com",
  "license": "ISC",
  "homepage": "https://dev.winston.services",
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
    "electron:start": "concurrently -k \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electronmon . --ignore=wallet.db/*\"",
    "electron:package:mac": "npm run build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "npm run build && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run start",
    "ionic:build:android": "npm run build && ionic build --platform android",
    "ionic:build:ios": "npm run build && ionic build --platform ios"
  },
  "dependencies": {
    "@capacitor/android": "6.1.2",
    "@capacitor/app": "6.0.1",
    "@capacitor/cli": "6.1.2",
    "@capacitor/core": "6.1.2",
    "@capacitor/haptics": "6.0.1",
    "@capacitor/ios": "6.1.2",
    "@capacitor/keyboard": "6.0.2",
    "@capacitor/status-bar": "6.0.1",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/base": "^5.0.0-alpha.76",
    "@mui/icons-material": "^5.5.0",
    "@mui/lab": "^5.0.0-alpha.73",
    "@mui/material": "^5.5.0",
    "@mui/styles": "^5.5.0",
    "@mui/x-date-pickers": "^5.0.0-alpha.1",
    "@reduxjs/toolkit": "^1.8.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "ethers": "^6.13.2",
    "formik": "^2.2.9",
    "html-to-draftjs": "^1.5.0",
    "immutability-helper": "^3.1.1",
    "js-file-download": "^0.4.12",
    "level": "^8.0.1",
    "node-rsa": "^1.1.1",
    "qrcode-npm": "^0.0.3",
    "react": "^17.0.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^17.0.2",
    "react-draft-wysiwyg": "^1.14.7",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^5.0.0",
    "react-slick": "^0.28.1",
    "recharts": "^2.1.9",
    "slick-carousel": "^1.8.1",
    "source-map-explorer": "^2.5.2",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "concurrently": "^7.0.0",
    "constants-browserify": "^1.0.0",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "electron": "^17.1.2",
    "electron-builder": "^25.0.5",
    "electron-is-dev": "^2.0.0",
    "electronmon": "^2.0.2",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "vm-browserify": "^1.1.2",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "com.electron.winston",
    "productName": "Winston",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  }
}
